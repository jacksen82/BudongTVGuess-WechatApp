.result {
  display: block;
  position: fixed;
  top: 0px;
  left: 20px;
  right: 20px;
  height: 100px;
}
.result em {
  display: block;
  height: 42px;
  color: #db2863;
  font-size: 12pt;
  font-weight: bold;
  line-height: 42px;
  text-align: center;
  border-radius: 21px;
  background: #fad759;
}
.result dfn {
  display: block;
  color: #f2c3d4;
  font-size: 11pt;
  text-align: center;
  line-height:48px;
}
.result dfn strong {
  color: #ffffff;
  font-weight: bold;
}
.clientlist {
  display: block;
  overflow: auto;
  position: fixed;
  top: 100px;
  left: 20px;
  right: 20px;
  bottom: 100px;
  padding: 0px;
  -webkit-overflow-scrolling: touch;
}
.clientlist .item {
  display: block;
  padding:3px 0px;
  margin-bottom: 1px;
  background: rgba(0,0,0,.1);
}
.clientlist .item .index {
  float: left;
  display: block;
  width: 36px;
  height: 36px;
  color:#f2c3d4;
  font-weight: bold;
  text-align: center;
  line-height: 36px;
}
.clientlist .item .avatar {
  float: left;
  display: block;
  width: 36px;
  height: 36px;
  border-radius: 18px;
}
.clientlist .item .nick {
  display: block;
  height: 36px;
  color: #f2c3d4;
  font-size: 9pt;
  line-height: 36px;
  padding-left: 10px;
  margin: 0px 96px 0px 72px;
}
.clientlist .item .score {
  float: right;
  display: block;
  width: 36px;
  height: 36px;
  color: #ffffff;
  font-size: 9pt;
  text-align: right;
  line-height: 36px;
}
.clientlist .item .time {
  float: right;
  display: block;
  width: 36px;
  height: 36px;
  color: rgba(255,255,255,0.8);
  font-size: 9pt;
  text-align: right;
  line-height: 36px;
  padding: 0px 12px;
}
.clientlist .item.me {
  background: transparent;
}
.clientlist .item.me .index {
  color: #fad759;
}
.clientlist .item.me .nick {
  color: #fad759;
  font-weight: bold;
}
.clientlist .item:last-child {
  border-bottom: 1px solid rgba(0,0,0,.1);
}
.controls {
  display: block;
  position: fixed;
  left: 35px;
  right: 35px;
  bottom: 0px;
  height: 70px;
  padding-top: 30px;
}
.controls .button {
  display: block;
  padding: 0px 3px 3px 3px;
  margin:0px auto;
  border-radius: 12px;
  background: #db2863;
}
.controls .button span {
  display: block;
  height: 40px;
  color: #db2863;
  font-size: 11pt;
  line-height: 40px;
  padding: 0px 20px;
  border-bottom: 3px solid #f2c3d4;
  border-radius: 10px;
  background: #fdecf2;
}
.controls .button.active {
  padding: 3px;
}
.controls .button.active span {
  border-bottom: 0px;
}
.controls .button.left {
  float: left;
  display: block;
}
.controls .button.right {
  float: right;
  display: block;
}