.television {
  display: block;
  margin: 0px auto;
}
.television .antenna {
  display: block;
  overflow: hidden;
  padding: 0px 15%;
}
.television .antenna var {
  display: block;
  height: 4px;
  border: 1px solid #d01651;
  border-radius:4px;
  background:#fdecf2;
}
.television .antenna var:before {
  display: block;
  content: "";
  width: 8px;
  height: 8px;
  margin-top: -3px;
  margin-left: -3px;
  border: 1px solid #d01651;
  border-radius: 50%;
  background:#fdecf2;
}
.television .antenna var:first-child {
  float:left;
  width: 36%;
  margin-top: 24%;
  transform: rotate(45deg);
}
.television .antenna var:last-child {
  float:right;
  width: 48%;
  margin-top: 24%;
  transform: rotate(135deg);
}
.television .shell {
  display:block;
  padding: 0px;
  margin: 0px auto;
  border: 2px solid #d01651;
  border-radius:16px;
  background:#fe4f86;
}
.television .shell .button {
  float: right;
  display: block;
  padding: 0px;
}
.television .shell .button dfn {
  display: block;
  margin: 5px auto 0px auto;
  border: 2px solid #d01651;
  border-radius: 50%;
  background:#fdecf2;
}
.television .shell .screen {
  display: block;
  overflow: hidden;
  border: 2px solid #d01651;
  border-radius:12px;
  background:#fdecf2;
}